#include <iostream>
#include <vector>
using namespace std;


int main(void){

	int a = 34747474;
	if(a<8) {
		cout << "less\n"
	}
	else {
		cout << "waaaaau\n"
	}

	a=4;
	if(a==4){
		cout << "good\n";
	}

	if(a>3) {
		cout<<"more\n";	
	
	}
	
	string b="ahoj";

	if(b=="ahoj"){
		cout << "string ok in if statement\n"'
	}

	if(a<=3){
		cout << "this never happen\n";
	}
	else{
		cout<<"else statement OK\n";
	}

	if(a>=4){
		cout << "better\n";
	}

	
	return 0;
}
